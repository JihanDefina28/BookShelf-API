{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "eol-last": ["error", "never"],
        "no-console": "off",
        "linebreak-style": "off",
        "no-multiple-empty-lines": [2, {"max": 99999, "maxEOF": 0}]
    }
}
